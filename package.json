{
  "name": "dbl-functions",
  "version": "1.0.0",
  "scripts": {
    "env:dev": "cp ./config/development/.env.dev .env && cp ./config/development/service-account.json ./src/services/firebase/service-account.json && firebase use development",
    "env:prod": "cp ./config/development/.env.prod .env && cp ./config/production/service-account.json ./src/services/firebase/service-account.json && firebase use production",
    "dev": "ts-node-transpile-only",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "firebase serve --only functions",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "eslint --ext [.ts,.tsx] .",
    "lint:prettier": "prettier -c \"**/*.{ts,tsx}\"",
    "lint:tsc": "tsc",
    "test": "jest",
    "predeploy": "yarn lint && rimraf ./dist && yarn run build",
    "deploy": "firebase deploy --only functions"
  },
  "main": "dist/index.js",
  "dependencies": {
    "axios": "^0.26.1",
    "bip32": "^3.0.1",
    "bip39": "^3.0.4",
    "bitcoinjs-lib": "^6.0.1",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.0",
    "ecpair": "^2.0.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.19.0",
    "moment": "^2.29.1",
    "tiny-secp256k1": "^2.2.1",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "add": "^2.0.6",
    "babel-jest": "^27.5.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.3",
    "firebase-tools": "^10.5.0",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0"
  },
  "resolutions": {
    "@types/serve-static": "1.13.9"
  },
  "private": true
}
